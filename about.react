function renderTeamCard(name, role, desc, img) {
  return (
    <div key={name} className="team-card" style={{borderRadius:18,overflow:'hidden'}}>
      <img alt={`Portrait of ${name}`} src={img} />
      <div className="overlay">
        <div className="role">{role}</div>
        <h3>{name}</h3>
        <p style={{color:'#cbd5e1'}}>{desc}</p>
      </div>
    </div>
  );
}
