function injectAssets() {
  // inject CSS
  const style = document.createElement('style');
  style.setAttribute('data-injected', 'next-tech-coder-styles');
  style.innerHTML = INJECTED_CSS;
  document.head.appendChild(style);

  // inject Google Fonts and Material Symbols (only once)
  if (!document.getElementById('ntc-fonts')) {
    const link = document.createElement('link');
    link.id = 'ntc-fonts';
    link.href = "https://fonts.googleapis.com/css2?family=Space+Grotesk:wght@300;400;500;600;700&family=Noto+Sans:wght@400;500;600;700&display=swap";
    link.rel = 'stylesheet';
    document.head.appendChild(link);
  }

  if (!document.getElementById('ntc-material')) {
    const link2 = document.createElement('link');
    link2.id = 'ntc-material';
    link2.href = "https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:wght,FILL@100..700,0..1&display=swap";
    link2.rel = 'stylesheet';
    document.head.appendChild(link2);
  }
}

